      "astype",
      "bdist",
      "bpython",
      "datetime",
      "dgraphpandas",
      "dropna",
      "dtype",
      "fillna",
      "inplace",
      "numpy",
      "pformat",
      "sdist",
      "setuptools",
      "startify",
      "tolist",
      "upserts",
      "vsplit"
    ],
    "cSpell.showStatus": false,
    "cSpell.maxNumberOfProblems": 100
}
