{
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.jediEnabled": false,
  "explorer.width": 30,
  "explorer.previewAction.onHover": false,
  "explorer.keyMappings.global": {
    "<cr>": [
      "expandable?",
      "expand",
      "open"
    ],
    "v": "open:vsplit"
  },
