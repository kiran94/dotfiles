    "DGRAPH",
    "Floaterm",
    "PYPI",
    "Upsert",
    "astype",
    "bdist",
    "bpython",
    "codecov",
    "datetime",
    "dgraphpandas",
    "dropna",
    "dtype",
    "fillna",
    "inplace",
    "ipads",
    "isin",
    "kwargs",
    "noqa",
    "numpy",
    "omnisharp",
    "pformat",
    "pydocstring",
    "pytest",
    "sdist",
    "setuptools",
    "startify",
    "tolist",
    "upserts",
    "vsplit"
  ],
  "cSpell.showStatus": false,
  "cSpell.maxNumberOfProblems": 100
}
